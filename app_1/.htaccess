RewriteEngine On
RewriteRule ^js/ - [L]
RewriteRule ^css/ - [L]
RewriteRule ^images/ - [L]
RewriteRule ^fonts/ - [L]
RewriteRule ^favicon/ - [L]
RewriteRule ^uploads/ - [L]
RewriteRule ^public/ - [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^index\.php$ index.php [L]
RewriteRule ^(.*)$ index.php [L]